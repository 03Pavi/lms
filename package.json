{
  "name": "attendence_manager-main",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "docker compose up",
    "start": "cross-env node server.js 8080",
    "start:dev": "cross-env nodemon server.js 8080",
    "db:create": "cross-env npx sequelize-cli db:create",
    "db:drop": "cross-env npx sequelize-cli db:drop",
    "up": "cross-env npx sequelize-cli db:migrate",
    "undo": "cross-env npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "dispatch-messages": "node ./message_bus/dispatch_messages.js",
    "handle-messages": "node ./workers/consumer.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^7.8.0",
    "amqplib": "^0.10.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mime-types": "^2.1.35",
    "nanoid": "^3.3.4",
    "nodemon": "^3.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "sequelize-cli": "^6.6.2",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  }
}
